{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "rootDir": "./src",
    "outDir": "./dist",
     "types": ["node", "express"],

    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": false,
    "skipLibCheck": true
  }
}
